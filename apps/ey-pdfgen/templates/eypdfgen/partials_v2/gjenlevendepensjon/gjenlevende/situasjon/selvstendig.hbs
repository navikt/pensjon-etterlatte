{{#if soeker.arbeidOgUtdanning.selvstendig.svar}}
    <div>
        <h4>
            {{#eq spraak "en"}}
                About your business
            {{else}}
                {{#eq spraak "nn"}}
                    Om næringa
                {{else}}
                    Om næringen
                {{/eq}}
            {{/eq}}
        </h4>

        {{#with soeker.arbeidOgUtdanning.selvstendig.svar as | naeringer |}}
            {{#each naeringer}}
                <div class="{{#gt naeringer.length 1}}panel{{/gt}}">
                    {{> eypdfgen/partials_v2/common/opplysning this.firmanavn}}

                    {{> eypdfgen/partials_v2/common/opplysning this.orgnr}}

                    {{> eypdfgen/partials_v2/common/opplysning this.endretInntekt}}

                    {{#eq this.endretInntekt.svar.verdi "JA" }}
                        {{> eypdfgen/partials_v2/common/opplysning this.endretInntekt.opplysning}}
                    {{/eq}}
                </div>
            {{/each}}
        {{/with}}
    </div>
{{/if}}
