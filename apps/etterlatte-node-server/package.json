  "main": "server.js",
  "dependencies": {
    "@navikt/nav-dekoratoren-moduler": "2.1.5",
    "@navikt/oasis": "^3.2.2",
    "cls-rtracer": "^2.6.0",
    "express": "4.18.3",
    "jsdom": ">=24.x",
    "jsonwebtoken": "9.0.2",
    "node-cache": "5.1.2",
    "node-fetch": "2.7.0",
    "prom-client": "^15.1.0",
    "react": "^18.2.0",
    "typescript": "5.3.3",
    "uuid": "^9.0.1",
    "winston": "3.11.0",
    "winston-transport": "^4.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-http-proxy": "^1.6.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node-fetch": "^2.5.12",
    "@types/node-jose": "^1.1.13",
    "@types/uuid": "^9.0.8",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0"
  },
  "scripts": {
    "start": "ts-node-dev --project ./tsconfig.json src/server.ts",
    "start:gjenlevendepensjon": "BASE_PATH=/gjenlevendepensjon/soknad yarn start:mock",
    "start:barnepensjon": "BASE_PATH=/barnepensjon/soknad yarn start:mock",
    "start:omstillingsstonad": "BASE_PATH=/omstillingsstonad/soknad yarn start:mock",
    "start:mock": "ts-node-dev --project ./tsconfig.json src/mock/mock-server.ts",
    "build": "NODE_ENV=production tsc --project ./tsconfig.json",
    "build-dev": "NODE_ENV=dev tsc --project ./tsconfig.json",
    "test": "NODE_ENV=production tsc --project ./tsconfig.json"
  },
  "prettier": {
    "trailingComma": "es5",
    "printWidth": 120,
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true
  }
