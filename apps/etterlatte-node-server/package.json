{
  "name": "etterlatte-node-server",
  "version": "1.0.0",
  "author": "NAVIKT",
  "license": "MIT",
  "private": true,
  "main": "server.js",
  "dependencies": {
    "@navikt/nav-dekoratoren-moduler": "1.6.17",
    "cls-rtracer": "^2.6.0",
    "express": "4.17.1",
    "jsonwebtoken": "9.0.0",
    "node-cache": "5.1.2",
    "node-fetch": "^2.6.7",
    "node-jose": "2.0.0",
    "openid-client": "4.7.4",
    "prom-client": "^14.0.1",
    "typescript": "4.3.5",
    "uuid": "^8.3.2",
    "winston": "3.3.3",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-http-proxy": "^1.6.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node-fetch": "^2.5.12",
    "@types/node-jose": "^1.1.8",
    "@types/uuid": "^8.3.4",
    "prettier": "^2.5.1",
    "ts-node-dev": "^1.1.8"
  },
  "scripts": {
    "start": "ts-node-dev --project ./tsconfig.json src/server.ts",
    "start:gjenlevendepensjon": "BASE_PATH=/gjenlevendepensjon/soknad yarn start:mock",
    "start:barnepensjon": "BASE_PATH=/barnepensjon/soknad yarn start:mock",
    "start:mock": "ts-node-dev --project ./tsconfig.json src/mock/mock-server.ts",
    "build": "NODE_ENV=production tsc --project ./tsconfig.json",
    "build-dev": "NODE_ENV=dev tsc --project ./tsconfig.json"
  },
  "prettier": {
    "trailingComma": "es5",
    "printWidth": 120,
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true
  }
}
