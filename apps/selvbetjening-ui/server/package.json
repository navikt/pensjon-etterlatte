{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "main": "server.js",
  "dependencies": {
    "@navikt/nav-dekoratoren-moduler": "1.6.9",
    "express": "4.17.1",
    "express-http-proxy": "1.6.2",
    "jsonwebtoken": "8.5.1",
    "node-cache": "5.1.2",
    "node-fetch": "^2.6.7",
    "node-jose": "2.0.0",
    "npm": "^7.23.0",
    "openid-client": "4.7.4",
    "prom-client": "^14.0.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.3.5",
    "ulid": "2.3.0",
    "winston": "3.3.3",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-http-proxy": "^1.6.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node-fetch": "^2.5.12",
    "@types/node-jose": "^1.1.8",
    "@types/redis": "^2.8.32",
    "jest": "^27.4.7",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3"
  },
  "scripts": {
    "start": "NODE_ENV=dev ts-node-dev --project ./tsconfig.json src/server.ts",
    "start:mock": "ts-node-dev --project ./tsconfig.json src/mock/mock-server.ts",
    "start:prod": "NODE_ENV=production ts-node-dev --project ./tsconfig.json ./build/server.ts",
    "build": "NODE_ENV=production tsc --project ./tsconfig.json",
    "build-dev": "NODE_ENV=dev tsc --project ./tsconfig.json",
    "test": "node --experimental-vm-modules  node_modules/.bin/jest --config ./jest.config.ts --forceExit",
    "test:watch": "node --experimental-vm-modules  node_modules/.bin/jest --config ./jest.config.ts --watch"
  },
  "type": "module"
}
