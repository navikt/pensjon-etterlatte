{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "main": "server.js",
  "dependencies": {
    "@navikt/nav-dekoratoren-moduler": "1.6.9",
    "express": "4.17.1",
    "express-http-proxy": "1.6.2",
    "jsonwebtoken": "8.5.1",
    "node-cache": "5.1.2",
    "node-fetch": "^2.6.7",
    "node-jose": "2.0.0",
    "npm": "^7.23.0",
    "openid-client": "4.7.4",
    "prom-client": "^14.0.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.3.5",
    "ulid": "2.3.0",
    "winston": "3.3.3",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-http-proxy": "^1.6.3",
